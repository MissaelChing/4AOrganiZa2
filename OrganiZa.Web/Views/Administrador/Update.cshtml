@model OrganiZa.Domain.DTOs.AdministradorRequestDto
@{
   

}
<form class="mt-3" method="post">
@using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
    <div class="row">
        <div class="col-md-12">
            <br>
            <br>
        </div>
    </div>
    <div class="row">

        <div class="col-md-12">
            <div class="form-group">
                <label>Nombre del Tutor</label>
                @Html.EditorFor(model => model.NombreAd, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group">
                <label>Tipo de cuenta</label>
                @Html.EditorFor(model => model.Rolusuario, new { htmlAttributes = new { @class = "form-control", disabled = true } })
            </div>
        </div>
        <div class="col-md-6 text-center">
            <div class="form-group">
                <a asp-controller="Home" asp-action="Home" class="btn btn-primary col-md-2">Volver</a>
            </div>
        </div>
        <div class="col-md-6 text-center">
            <div class="form-group">
                <input type="submit" value="Actualizar"class="btn btn-success col-md-2" />
            </div>
        </div>
    </div>
}
</form>


