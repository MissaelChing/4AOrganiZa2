@model OrganiZa.Domain.DTOs.EscuelaResponseDto
@{
    Layout = null;

}
<link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" />
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<html>

<head>



</head>

<body>

    <form class="mt-3" method="post">
        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <h1>@Model.NombreE</h1>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
        </div>
        <div class="row">

            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Colegiatura</label>
                    @Html.EditorFor(model => model.Colegiatura, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Modo de pago</label>
                    @Html.EditorFor(model => model.ModoP, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Codigo del administrador que creo la escuela</label>
                    @Html.EditorFor(model => model.IdA, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
            <div class="col-md-6 text-center">
                <div class="form-group">
                    <a asp-controller="Administrador" asp-action="Menu" class="btn btn-primary col-md-2">Volver</a>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                </div>
            </div>
        </div>
    </form>

</body>

</html>
